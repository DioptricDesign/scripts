!rxvt Unicode
URxvt.letterSpace:1
URxvt.font: xft:Monospace :size=12
URxvt.perl-ext-common:default,matcher,resize-font,bell-command,
URxvt.clipboard.autocopy: true
URxvt.url-launcher: /usr/bin/xdg-open
URxvt.matcher.button: 1
URxvt.scrollBar: false
URxvt*scrollBar_right:true
URxvt.iso14755: false
URxvt.iso14755_52: false
URxvt.internalBorder: 15
URxvt.cursorBlink: 1
URxvt*geometry: 70x17
URxvt.urgentOnBell: True
urxvt*depth: 32
URxvt*foreground: rgb:F4/F4/B9
URxvt*cursorColor: #7a7ab0
URxvt.keysym.CTRL++:  resize-font:smaller
URxvt.keysym.CTRL+-:    resize-font:bigger
